<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/item_history_nosender_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="18dp"
        android:background="@drawable/ic_bg_received"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <TextView
            android:id="@+id/item_history_nosender_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="부족하면 부족한대로 채우고 충분하면 충분한대로 매력 발산하면서 멋지게 살자.부족하면 부족한대로 채우고 충분하면 충분한대로 매력 발산하면서 멋지게 살자."
            android:maxLines="2"
            android:ellipsize="end"
            android:lineSpacingExtra="5dp"
            android:textColor="@color/black"
            android:fontFamily="@font/notosanskr_regular"
            android:textSize="14sp"
            android:layout_marginTop="25dp"
            android:layout_marginStart="34dp"
            android:layout_marginEnd="34dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <TextView
            android:id="@+id/item_history_nosender_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2021.12.20"
            android:textColor="@color/black"
            android:fontFamily="@font/notosanskr_regular"
            android:textSize="14sp"
            android:layout_marginTop="5dp"
            app:layout_constraintEnd_toEndOf="@id/item_history_nosender_content"
            app:layout_constraintTop_toBottomOf="@id/item_history_nosender_content"/>

        <TextView
            android:id="@+id/item_history_nosender_sender"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="18dp"
            android:text="처음이"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/item_history_nosender_date"
            app:layout_constraintTop_toBottomOf="@id/item_history_nosender_date" />

        <ImageView
            android:id="@+id/item_history_nosender_emotion"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:src="@drawable/emotion1"
            android:layout_marginEnd="5dp"
            app:layout_constraintEnd_toStartOf="@id/item_history_nosender_date"
            app:layout_constraintTop_toTopOf="@id/item_history_nosender_date"/>

        <ImageView
            android:id="@+id/item_history_nosender_done"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:src="@drawable/emotion5"
            android:layout_marginEnd="5dp"
            app:layout_constraintEnd_toStartOf="@id/item_history_nosender_emotion"
            app:layout_constraintTop_toTopOf="@id/item_history_nosender_emotion"/>


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>